{
  "version": "2.0",
  "extensions": {
    "cosmosDB": {
      "connectionMode": "Gateway",
      "userAgentSuffix": "CSV-Writer"
    },
    "blobs": {
        "maxDegreeOfParallelism": 4
      }
    },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}